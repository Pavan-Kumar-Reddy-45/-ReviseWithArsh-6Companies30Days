string encode(string src){
    string ans = "";
    int n = src.length();
    int i = 0;
    while(i < n){
        char ele = src[i];
        i++;
        int cnt = 1;
        while(i < n && src[i] == ele){
            cnt++;
            i++;
        }
        ans += ele;
        ans += to_string(cnt);
    }
    return ans;
} 
